    "test:bundlesize": "yarn run build:dist && bundlesize",
    "test:e2e": "NODE_ENV=test karma start --single-run",
    "test:lint": "eslint src/*.js test/**/*.js",
    "test:typecheck": "flow",
    "watch": "run-p watch:*",
